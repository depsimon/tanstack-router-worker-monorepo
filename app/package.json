		"@internationalized/date": "catalog:",
		"@lingui/core": "catalog:",
		"@lingui/react": "catalog:",
		"@orpc/client": "catalog:",
		"@orpc/contract": "catalog:",
		"@orpc/server": "catalog:",
		"@orpc/tanstack-query": "catalog:",
		"@acme/data": "workspace:",
		"@tailwindcss/vite": "catalog:",
		"@tanstack/devtools-vite": "catalog:",
		"@tanstack/react-devtools": "catalog:",
		"@tanstack/react-form": "catalog:",
		"@tanstack/react-form-devtools": "catalog:",
		"@tanstack/react-query": "catalog:",
		"@tanstack/react-query-devtools": "catalog:",
		"@tanstack/react-router": "catalog:",
		"@tanstack/react-router-devtools": "catalog:",
		"@tanstack/router-plugin": "catalog:",
		"arktype": "catalog:",
		"better-auth": "catalog:",
		"hono": "catalog:",
		"next-themes": "catalog:",
		"react-aria-components": "catalog:",
		"react": "catalog:",
		"react-dom": "catalog:",
		"sonner": "catalog:",
		"tailwind-merge": "catalog:",
		"tailwind-variants": "catalog:"
	},
	"devDependencies": {
		"@biomejs/biome": "catalog:",

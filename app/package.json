{
	"name": "@acme/app",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "lingui extract-template && vite build",
		"cf-typegen": "wrangler types --env-interface CloudflareBindings --env dev",
		"check": "biome check --write .",
		"deploy:dev": "bun run build --mode dev && wrangler deploy",
		"deploy:prod": "bun run build --mode production && wrangler deploy",
		"dev": "vite dev --port 3000",
		"lingui:extract": "lingui extract --clean",
		"tail": "wrangler tail --env dev",
		"typecheck": "tsc --noEmit"
	},
	"packageManager": "bun@1.3.0",
	"dependencies": {
		"@cloudflare/vite-plugin": "catalog:",
		"@hugeicons/core-free-icons": "catalog:",
